                    <PasswordBox Height="23" HorizontalAlignment="Left" Margin="163,130,0,0" Name="passwordBoxInscr" VerticalAlignment="Top" Width="120" />
                    <Label Content="Mail" Height="28" HorizontalAlignment="Left" Margin="41,173,0,0" Name="label16" VerticalAlignment="Top" />
                    <TextBox Height="23" HorizontalAlignment="Left" Margin="163,173,0,0" Name="textBoxInscrMail" VerticalAlignment="Top" Width="120" />
