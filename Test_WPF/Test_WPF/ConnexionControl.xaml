                    <Label Content="Année de naissance" Height="28" HorizontalAlignment="Left" Margin="41,345,0,0" Name="label14" VerticalAlignment="Top" />
                    <Button Content="Inscription" Height="Auto" HorizontalAlignment="Center" Margin="0,388,0,0" Name="button2" VerticalAlignment="Top" Width="Auto" Click="button2_Click" />
