             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             mc:Ignorable="d" 
             d:DesignHeight="43" d:DesignWidth="288">
    <Grid>
        <Label Content="Label" Height="Auto" HorizontalAlignment="Left" Margin="10,0,0,0" Name="lUsername" VerticalAlignment="Top" Width="Auto" Foreground="DarkMagenta" FontSize="24" />
